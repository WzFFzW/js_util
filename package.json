{
  "name": "js_util",
  "version": "1.0.0",
  "description": "js工具库",
  "main": "  ",
  "scripts": {
    "test": "ava",
    "build": "webpack --config ./script/webpack.conf.js --color",
    "dev": "webpack-dev-server --config ./script/webpack.dev.js --open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WzFFzW/js_util.git"
  },
  "keywords": [
    "js",
    "工具"
  ],
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "inherit": true,
    "verbose": true,
    "babel": {
			"testOptions": {
				"babelrc": false
			}
		}
  },
  "author": "WzFFzW",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WzFFzW/js_util/issues"
  },
  "homepage": "https://github.com/WzFFzW/js_util#readme",
  "devDependencies": {
    "@babel/register": "^7.4.4",
    "ava": "^1.4.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
